{
  "name": "larynx-sdk",
  "version": "2.0.0",
  "scripts": {
    "test": "npm run build_tests && npm run execute_tests",
    "execute_tests": "tslint -c tslint.json 'tests/**/*.ts' && mocha --opts tests/mocha.opts build/testBuild/**/*-test.js",
    "build": "tslint -c tslint.json 'src/**/*.ts' && rm -rf build/ && tsc -p . ",
    "build_tests": "tslint -c tslint.json 'src/**/*.ts' && rm -rf build/ && tsc -p ./tests/ ",
    "clean": "rm -rf build/ && rm -rf dist/",
    "prepublish": "npm run clean && npm run build "
  },
  "main": "./build/larynx.js",
  "types": "./build/larynx.d.ts",
  "license": "SEE LICENSE IN License.txt",
  "files": [
    "/build/larynx*"
  ],
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.11",
    "@types/pug": "^2.0.4",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "mocha-clean": "^1.0.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^1.3.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "typings": "^2.1.0"
  },
  "dependencies": {
    "pug": "^2.0.0-beta11",
    "xml2json": "^0.11.0"
  }
}
