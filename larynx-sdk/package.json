{
  "name": "larynx-sdk",
  "version": "2.0.0",
  "scripts": {
    "test": "npm run build && npm run execute_tests",
    "execute_tests": "tslint -c tslint.json 'tests/**/*.ts' && npm run build && mocha --opts tests/mocha.opts build/tests/**/*-test.js",
    "build": "tslint -c tslint.json 'src/**/*.ts' && rm -rf build/ && tsc -p . ",
    "clean": "rm -rf build/",
    "prepublish": "npm run clean && npm run build && npm run execute_tests"
  },
  "main": "build/src/index.js",
  "typings": "build/src/index.d.ts",
  "license": "SEE LICENSE IN License.txt",
  "files": [
    "/"
  ],
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.11",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "mocha-clean": "^1.0.0",
    "ts-node": "^1.3.0",
    "tslint": "^4.5.1",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "typings": "^2.1.0"
  }
}
